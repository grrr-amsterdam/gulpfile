{
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        },
        "allowImportExportEverywhere": true,
        "codeFrame": false
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
        "import"
    ],
    "rules": {
        "no-empty-label": 0,
        "space-after-keywords": 0,
        "space-return-throw-case": 0,
        "space-in-parens": 1,
        "keyword-spacing": 1,
        "no-labels": 1,
        "no-mixed-spaces-and-tabs": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "indent": 0,
        "quotes": 0,
        "space-before-function-paren": 0,
        "comma-dangle": 0,
        "no-plusplus": 0,
        "arrow-parens": 0,
        "spaced-comment": 0,
        "no-trailing-spaces": 1,
        "no-multi-spaces": 0,
        "no-param-reassign": 1,
        "padded-blocks": 1,
        "no-shadow": 1,
        "prefer-arrow-callback": 1,
        "no-use-before-define": [
          "error", {
            "functions": false,
            "classes": true
          }
        ],
        "arrow-body-style": 0,
        "consistent-return": 0,
        "import/no-unresolved": [
            "error", {
                "commonjs": true,
                "amd": true
            }
        ],
        "import/named": 2,
        "import/namespace": 2,
        "import/default": 2,
        "import/export": 2,
        "import/no-mutable-exports": 2,
        "import/first": 2,
        "import/no-unassigned-import": 2,
        "import/no-named-default": 0,
        "import/no-duplicates": 0,
        "import/prefer-default-export": 0
    }
}
